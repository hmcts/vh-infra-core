#!groovy
//noinspection GroovyUnusedAssignment
@Library("Infrastructure") _

//noinspection GroovyAssignabilityCheck
properties([
		parameters([
			choice(name: 'ENVIRONMENT', choices: 'sandbox', description: 'Environment where code should be build and deployed')
		])
])

slackChannel = '#vh-builds'

withParameterizedInfraPipeline('vh', params.ENVIRONMENT, 'sandbox') {
    enableSlackNotifications('#vh-builds')
}